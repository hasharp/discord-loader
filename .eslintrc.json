{
    "extends": [
        "eslint:recommended"
    ],
    "plugins": [],
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "es6": true,
        "node": true
    },
    "globals": {},
    "rules": {
        "no-unsafe-negation": 2,
        "valid-jsdoc": [2, {
            "requireReturn": false
        }],

        "array-callback-return": 1,
        "block-scoped-var": 2,
        "class-methods-use-this": 1,
        "consistent-return": 2,
        "curly": [1, "multi-line"],
        "dot-location": [1, "property"],
        "dot-notation": 2,
        "eqeqeq": 2,
        "guard-for-in": 1,
        "no-caller": 2,
        "no-eq-null": 0,
        "no-extend-native": 2,
        "no-extra-bind": 1,
        "no-extra-label": 1,
        "no-global-assign": 2,
        "no-implicit-coercion": [1, {"allow": ["!!", "~"]}],
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-iterator": 1,
        "no-lone-blocks": 1,
        "no-loop-func": 2,
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-new-wrappers": 1,
        "no-new": 1,
        "no-octal-escape": 2,
        "no-octal": 2,
        "no-proto": 1,
        "no-return-assign": 1,
        "no-return-await": 1,
        "no-script-url": 2,
        "no-self-compare": 1,
        "no-sequences": 1,
        "no-throw-literal": 1,
        "no-unmodified-loop-condition": 2,
        "no-unused-expressions": 2,
        "no-useless-call": 1,
        "no-useless-concat": 1,
        "no-useless-escape": 2,
        "no-useless-return": 1,
        "no-void": 1,
        "no-with": 2,
        "radix": 2,
        "vars-on-top": 2,
        "wrap-iife": [1, "inside"],

        "no-use-before-define": 2,

        "callback-return": 1,
        "global-require": 1,
        "handle-callback-err": 1,
        "no-mixed-requires": 2,
        "no-new-require": 1,
        "no-path-concat": 1,

        "array-bracket-spacing": 1,
        "block-spacing": 1,
        "brace-style": 1,
        "camelcase": 1,
        "comma-dangle": [1, "always-multiline"],
        "comma-spacing": 1,
        "comma-style": 1,
        "computed-property-spacing": 1,
        "consistent-this": 1,
        "func-call-spacing": 1,
        "func-name-matching": 1,
        "func-names": [1, "as-needed"],
        "func-style": [1, "declaration"],
        "key-spacing": 1,
        "keyword-spacing": [1, {
            "overrides": {
                "class": {
                    "after": false
                }
            }
        }],
        "new-cap": 2,
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-lonely-if": 2,
        "no-mixed-operators": 2,
        "no-new-object": 2,
        "no-unneeded-ternary": 1,
        "no-whitespace-before-property": 1,
        "object-curly-newline": [1, {
            "ObjectExpression": {
                "minProperties": 1
            },
            "ObjectPattern": "never"
        }],
        "object-curly-spacing": 1,
        "object-property-newline": 1,
        "operator-assignment": 1,
        "operator-linebreak": [1, "before"],
        "padded-blocks": [1, "never"],
        "quote-props": [1, "as-needed"],
        "quotes": [1, "single"],
        "semi-spacing": 1,
        "semi": 2,
        "space-before-blocks": 1,
        "space-before-function-paren": [1, "never"],
        "space-in-parens": 1,
        "space-infix-ops": 1,
        "space-unary-ops": [2, {
            "words": true,
            "nonwords": false,
            "overrides": {
                "new": false
            }
        }],

        "arrow-body-style": 1,
        "arrow-parens": [1, "as-needed"],
        "arrow-spacing": 1,
        "generator-star-spacing": 1,
        "no-duplicate-imports": 1,
        "no-useless-computed-key": 1,
        "no-useless-constructor": 1,
        "no-useless-rename": 1,
        "no-var": 2,
        "prefer-arrow-callback": 1,
        "prefer-const": 1,
        "prefer-numeric-literals": 1,
        "prefer-rest-params": 1,
        "prefer-spread": 1,
        "rest-spread-spacing": 1,
        "symbol-description": 1,
        "template-curly-spacing": 1,
        "yield-star-spacing": 1,

        "eol-last": 1,
        "indent": [1, 4, {
            "SwitchCase": 1
        }],
        "linebreak-style": [1, "unix"],
        "no-tabs": 1,
        "no-trailing-spaces": 1,
        "unicode-bom": 1
    }
}